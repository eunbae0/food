{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "liked": {
      "type": "integer",
      "default": 0
    },
    "thumbNail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "time": {
      "type": "datetime"
    },
    "desc": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "userId": {
      "type": "integer"
    }
  }
}
